	@Content.Script("jquery.svg-1.5.min.js", Url)
	@Content.Script("jquery.svgdom-1.5.min.js", Url)
	@Content.Script("jquery.cookie.js", Url)
	@Content.Script("mootools-core-1.4.1.js", Url)


	@Content.Script("jquery.tools.min-tips.js", Url)
	@Content.Script("register-tooltips.js", Url)

	@Content.Script("jquery.flash-message.js", Url)

	@Content.JSImport("~/JS/layoutImport", Url)

	
	
	@*@Content.Script("ui/GameWindowTabs.js", Url)
	@Content.Script("steg2d/StarSystemObjects.js", Url)
	@Content.Script("steg2d/StarSystemLoader.js", Url)
