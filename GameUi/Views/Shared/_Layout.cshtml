<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
	<title>@ViewBag.Title</title>
    <meta name="description" content="Space Traffic - Space trading multiplayer browser game." />


	<link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
	
	<!--jinak mi nešlo nalinkovat CSSko-->
	<link href="@Url.Content("~/Content/less/Account.less")" rel="stylesheet" type="text/css" />
    <link href='https://fonts.googleapis.com/css?family=Exo:400,700,800|Audiowide&subset=latin,latin-ext' rel='stylesheet' type='text/css'>


	@RenderSection("LinkStyles", false)
	<style type="text/css">
	@RenderSection("Styles", false)
	</style>
</head>

<body>
    <h1 id="layout-title">Space Traffic</h1>
	@Html.Partial("_Flash")
