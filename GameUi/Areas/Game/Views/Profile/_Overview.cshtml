@{
    ViewBag.Title = "_Overview";
}

<div class="profile_wrapper">
    <div id="profile_experience">
        <canvas id="exp_ring" width="338" height="338"></canvas>
        <!--<div id="exp_text">@ViewBag.player.Experiences / @ViewBag.nextLevelExp</div>-->
        <div id="exp_text">Potřebných zkušeností: xxx</div>
    </div>
    <div id="profile_info">
        <h1>Informace</h1>
        <p>
            <span class="profile_p_title">Jméno:</span> @ViewBag.player.FirstName
        </p>
        <p>
            <span class="profile_p_title">Úroveň:</span> @ViewBag.currentLevel.LevelID - @ViewBag.currentLevel.Name
        </p>
        <p>
            <span class="profile_p_title">Zkušenosti:</span> @ViewBag.player.Experiences
        </p>
        <p>
            <span class="profile_p_title">Věk:</span>
        </p>
        <p>
            <span class="profile_p_title">Info:</span>
        </p>
    </div>
</div>
