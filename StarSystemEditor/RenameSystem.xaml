        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename Star System" Height="123" Width="300">
    <Grid Height="89">
        <Label Content="New name:" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="139,14,0,0" Name="name_text" VerticalAlignment="Top" Width="120" Text="NewStarSystem" TextAlignment="Center" />
        <Button Content="Confirm" Height="23" HorizontalAlignment="Left" Margin="184,53,0,0" Name="Confirm_button" VerticalAlignment="Top" Width="75" Click="Confirm_button_Click" />
    </Grid>
</Window>
