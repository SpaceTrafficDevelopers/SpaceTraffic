@{
    ViewBag.Title = "QunitTests";
}
@section LinkStyles {
    <link href="http://code.jquery.com/qunit/git/qunit.css" rel="stylesheet" type="text/css" />
}

<h1 id="qunit-header">Space Traffic Qunit tests</h1>
<h2 id="qunit-banner"></h2>
<div id="qunit-testrunner-toolbar"></div>
<h2 id="qunit-userAgent"></h2>
<ol id="qunit-tests"></ol>
<div id="qunit-fixture">test markup, will be hidden</div>


@section Scripts {
    <script type="text/javascript" src="http://code.jquery.com/qunit/git/qunit.js"></script>
    @Content.STJS("map/SvgStarSystemMap.Tests.js", Url)
    @Content.STJS("map/StarSystemObjects.Tests.js", Url)
    @Content.STJS("tests/JQuerySvgDomTest.js", Url)
}